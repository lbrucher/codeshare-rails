
<div id='sessions'>
	<div id='open'>
		Open sessions (<%= link_to "Create New", "interviewer#createNew", :id => 'create' %>)
	<div>
	<table border='0' cellspacing='0' cellpadding='4'>
		<% @openSessions.each do |se| %>
		<tr>
			<td>[ <span id='sessionId'><%= se.code %></span> ]</td>
			<td><%=se.created_at%></td>
			<td>
				<a href='/interviewer/session/<%=se.id%>/details'>Details</a>
				&nbsp;|
				<a href='/interviewer/session/<%=se.id%>'>Resume</a>
				&nbsp;|
				<a href='#'>Close</a>
				&nbsp;|
				<a href='#'>Delete</a>
			</td>
		</tr>
		<% end %>
	</table>

	<div id='closed'>Closed sessions</div>
	<table border='0' cellspacing='0' cellpadding='4'>
		<% @closedSessions.each do |se| %>
		<tr>
			<td>[<%=se.code%>]</td>
			<td><%=se.created_at%></td>
			<td>
				actions...
			</td>
		</tr>
		<% end %>
	</table>
</div>
